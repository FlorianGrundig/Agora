extends ../../../views/layout

block title
  | Wiki

block content
  .page-header
    h2 Vergleich von "#{pageName}"

  table.compare.table.table-bordered.table-hover.table-condensed
    each line in lines
      tr
        td.ln #{line.ldln}
        td.ln #{line.rdln}
        td(class="#{line.class}") #{line.text}
